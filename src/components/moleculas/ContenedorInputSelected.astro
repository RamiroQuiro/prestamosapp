---
import LabelForm from "../atomos/LabelForm.astro";
import SelectedFormulario from "../atomos/SelectedFormulario.astro";

const {label,options,name,labelOption}=Astro.props
---


<div class="w-full flex items flex-col items-start gap-1 duration-300 group -md p-3">
   <LabelForm>{label}</LabelForm>
      <SelectedFormulario
      labelOption={labelOption}
      options={options}
      name={name}
      />

  </div>
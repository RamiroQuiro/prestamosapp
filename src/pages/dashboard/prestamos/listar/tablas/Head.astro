---
import Th from "../../../../../components/atomos/Th.astro";
import Tr from "../../../../../components/atomos/Tr.astro";

const { cabecera } = Astro.props;
---

<thead class="bg-primary-100/70 rounded-md text-white">
  <Tr styleTr="rounded-md">
    {
      cabecera?.map((cabe) => {
        return <Th> {cabe} </Th>;
      })
    }
  </Tr>
</thead>

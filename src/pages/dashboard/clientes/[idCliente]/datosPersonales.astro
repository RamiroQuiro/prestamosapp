---
import DatosPersonales from './DatosPersonales'
import { clientes as Cliente } from '@/db/schema';
import LayoutClient from './LayoutClient.astro';
import db from '@/db';
import { eq } from 'drizzle-orm';
const { idCliente } = Astro.params;

let clienteDB;
try {
  clienteDB = (
    await db.select().from(Cliente).where(eq(Cliente.id, idCliente))
  ).at(0);
 
} catch (error) {
  console.log(error);
}
---

<LayoutClient>
    <DatosPersonales cliente={clienteDB} client:load />
</WraperCliente>